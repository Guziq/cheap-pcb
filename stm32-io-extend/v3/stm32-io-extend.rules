
(rules PCB stm32-io-extend
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 10696)
    (layer_rule F.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 200.0)
    (clear 200.2)
    (clear 100.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_400:200_um"
    (shape
      (circle F.Cu 400.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 400.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_400:200_um" "Via[0-1]_400:200_um" default
  )
  (via 
    "Via[0-1]_400:200_um-kicad_default" "Via[0-1]_400:200_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_400:200_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_400:200_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +5V +3V3 /NRST "Net-(J1-PadB5)" "Net-(J1-PadA6)" "Net-(J1-PadA5)" "Net-(J1-PadA7)"
    "Net-(J2-PadB5)" "Net-(J2-PadA6)" "Net-(J2-PadA5)" "Net-(J2-PadA7)" "Net-(R1-Pad2)" "Net-(R2-Pad1)" "Net-(R5-Pad1)" /BOOT1
    /BOOT0 /USBDP "/USB_PULLUP" /USBDM "/USART1_TX" "/USART1_RX" /P1 /P2
    /P3 /P4 /P5 /P6 /P7 /P8 /P9 /P10
    /P11 /P12 /P13 /P14 /P15 /P16 /P17 /P18
    /P19 /P20 /P21 /P22 /P23 /P24 /P25 /P26
    /P27 /P28 /P29 "Net-(C13-Pad1)" "Net-(C14-Pad1)" "Net-(C6-Pad1)" "Net-(D1-Pad1)" "Net-(D2-Pad2)"
    "Net-(R6-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)